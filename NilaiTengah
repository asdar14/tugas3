// Example program
#include <iostream>
#include <string>

using namespace std;

int main()
{
  int A, B, C;
  cout << "========================";
  cout << "\nMenghitung Nilai Tengah";
  cout << "\n========================";
  
  cout << "\nMasukan Nilai Pertama :";
  cin >> A;
  cout << "\nMasukan Nilai Kedua :";
  cin >> B;
  cout << "\nMasukan Nilai Ketiga :";
  cin >> C;
  
  if (A < B && B < C)
    cout << "Nilai Tengah :" << B;
    else{
    if(C < B && B < A)
        cout << "Nilai Tengah :" << B;
        else{
        if(A < C && C < B)
            cout << "Nilai Tengah :" << C;
            else{
            if(B < C && C < A)
                cout << "Nilai Tengah :" << C;
                else{
                if(C < A && A < B)
                    cout << "Nilai Tengah :" << A;
                    else{
                    if(A > B && A < C)
                        cout << "Nilai Tengah :" << A;
                        else
                        cout << "Nila Tengah : -";
                    }    
                }        
            }            
        }
    }
    
}
